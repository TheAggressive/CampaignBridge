{
  "name": "campaignbridge",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "create-block": "npx @wordpress/create-block@latest --no-plugin --namespace campaignbridge",
    "create-block-dynamic": "npx @wordpress/create-block@latest --no-plugin --namespace campaignbridge --variant dynamic",
    "create-block-interactive": "npx @wordpress/create-block@latest --no-plugin --namespace campaignbridge --template @wordpress/create-block-interactive-template",
    "build:blocks": "wp-scripts build --config webpack.config.js --source-path=./src/blocks --output-path=./dist/blocks",
    "build:interactivity": "wp-scripts build --config webpack.config.js --source-path=./src/blocks-interactivity --output-path=./dist/blocks-interactivity --experimental-modules",
    "build:assets": "wp-scripts build --config webpack.assets.config.js",
    "build": "concurrently 'pnpm build:blocks' 'pnpm build:interactivity' 'pnpm build:assets'",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style 'src/**/*.css'",
    "lint:js": "wp-scripts lint-js ./src/**/*.js",
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip",
    "start:blocks": "wp-scripts start --config webpack.config.js --source-path=./src/blocks --output-path=./dist/blocks",
    "start:interactivity": "wp-scripts start --config webpack.config.js --source-path=./src/blocks-interactivity --output-path=./dist/blocks-interactivity --experimental-modules",
    "start:assets": "wp-scripts start --config webpack.assets.config.js",
    "start": "concurrently 'pnpm start:blocks' 'pnpm start:interactivity' 'pnpm start:assets'",
    "start:hot": "wp-scripts start --output-path=./dist --hot",
    "start:copy-php": "wp-scripts start --output-path=./dist --webpack-copy-php",
    "js:watch": "wp-scripts start ./src/scripts/app.js --output-path=dist/scripts"
  },
  "keywords": [],
  "author": "Aggressive Network, LLC",
  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@wordpress/annotations": "^3.28.0",
    "@wordpress/block-editor": "^15.1.0",
    "@wordpress/block-library": "^9.28.0",
    "@wordpress/blocks": "^15.1.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^6.28.0",
    "@wordpress/dom-ready": "^4.28.0",
    "@wordpress/scripts": "^30.21.0",
    "concurrently": "^9.2.0",
    "fast-glob": "^3.3.2",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sass": "^1.79.5",
    "tailwindcss": "^4.1.11",
    "webpack-merge": "^6.0.1",
    "webpack-remove-empty-scripts": "^1.1.1"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^7.28.0"
  }
}
